
# zig and release

GIT_DOMAIN=${GIT_DOMAIN:-$(x websrc gitdomain)}

function test_yy(){
    local num=$1
    x:info "正在准备测试 y$num"
    (
        git clone "git@${GIT_DOMAIN}:x-cmd/y$num"
        cd "y$num" || exit
        x yanfa yy test
    ) &
}

function main(){

    local i; for i in "$@"; do
        test_yy "$i" || exit 1
    done
}

if [ $# -eq 0 ]; then
    set -- 6 7
fi

main "$@"