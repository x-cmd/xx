function main(){
    local i=
    for i in "$@"; do
        x yanfa yy test "$i" || return 1
    done
}

if [ -n "$test_yy_version" ]; then
    set -- "$test_yy_version"
elif [ $# -eq 0 ]; then
    set -- 6 7
fi

main "$@"